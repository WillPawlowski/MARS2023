<launch>

   <node pkg="rplidar_ros" type="LaserScan" name="lidar" output="screen">
    <param name="lidar_description" command="$(find xacro)/xacro '$(find mars_robot)/description/lidar.xacro'" />
 </node>
 <node pkg="odrive_ros" type="odrive_node.py" name="odom_node" output="screen">
    <param name="odom_description" command="$(find xacro)/xacro '$(find mars_robot)/description/rover_core.xacro'" />
 </node>
 <node pkg="desciption" type="" name="transform_configuration_name" output="screen">
    <param name="transform_configuration_param" value="param_value" />
 </node>

</launch>
